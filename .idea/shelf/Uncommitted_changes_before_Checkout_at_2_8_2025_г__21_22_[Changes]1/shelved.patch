+        int number = 1;
+
+        while (true){
+            number++;
+
+            if (number%2==0){
+                System.out.println(number + " is even");
+            }
+            else {
+                System.out.println("This number is odd");
+                continue;
+            }
+
+            if (number==1002){
+                break;
+            }
+        }
+
+        System.out.println("--------------------------");
+
+        int numberTwo = 5;
